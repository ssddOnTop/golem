package api:todos@1.0.0;

interface address-service {
    use %type.{address, get-user-request, user};
    
    
    add-address: func(input: address) -> option<address>;
    get-address-for-user: func(input: user) -> option<address>;
}

interface %type {
    
    record address { city: string, street: string, user: user, zip-code: string }
    record get-user-request { id: string }
    record user { email: string, id: string, name: string }
    
    
}

interface user-service {
    use %type.{address, get-user-request, user};
    
    
    get-user: func(input: get-user-request) -> option<user>;
    create-user: func(input: user) -> option<user>;
}
